<div class="row">
  <div class="col-md-12">
    <div class="panel panel-success">
      <div class="panel-heading"></div>
      <div class="panel-body">

        <%= simple_form_for(@project) do |f| %>
            <%= f.error_notification %>

            <div class="form-inputs">
              <%= f.input :reference %>
              <%= f.input :object_project  %>
              <%= f.input :focal_point %>
              <%= f.input :project_situation %>
              <%= f.input :public_partenership %>
              <%= f.input :private_partenership %>
              <%= f.input :amount %>
              <%= f.input :duration %>
            </div>

            <div class="form-actions">
              <%= f.button :submit, class: 'btn btn-success' %>
            </div>
        <% end %>

      </div>
    </div>
  </div>
</div>