<p id="notice">
  <%= notice %>
</p>

<h1>Projects</h1>

<table class="table table-hover">
  <thead>
  <tr class="success">
    <th>Reference</th>
    <th>Objet du projet</th>
    <th>Point focal CARPA</th>
    <th>Situation du projet</th>
    <th>Partenaire Public</th>
    <th>Partenaire Privé</th>
    <th>Montant de l'investissement</th>
    <th>Durée du contrat</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @projects.each do |project| %>
      <tr>
        <td>
          <%= project.reference %>
        </td>
        <td>
          <%= project.object_project %>
        </td>
        <td>
          <%= project.focal_point %>
        </td>
        <td>
          <%= project.project_situation %>
        </td>
        <td>
          <%= project.public_partenership %>
        </td>
        <td>
          <%= project.private_partenership %>
        </td>
        <td>
          <%= project.amount %>
        </td>
        <td>
          <%= project.duration %>
        </td>

        <td>
          <%= link_to 'Show', project %>
        </td>
        <td>
          <%= link_to 'Edit', edit_project_path(project) %>
        </td>
        <td>
          <%= link_to 'Destroy', project, method: :delete, data: { confirm: 'Are you sure?' } %>
        </td>
      </tr>
  <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Project', new_project_path, class: 'btn btn-success' %>